#include <stdio.h>
#define tam 5
int main(int argc, char *argv[]) 
{
	int a[tam],j=0,aux=0,i=0,pos=0;
	for(i=0;i<tam;i++)
	{
		pos++;
		printf("Ingrese un numero en la posicion %d: ",pos);
		scanf("%d",&a[i]);
	}
	for(i=0;i<tam;i++)
	{
		printf("%d \n",a[i]);		
	}
	i=0;
	
	for(i=0;i<tam-1;i++)
	{
		for (j=0;j<(tam-1-i);j++)
		{
			if (a[j]>a[j+1])
			{
				aux = a [ j ];
				a[ j ] = a [ j + 1];
				a[ j + 1] = aux;
			}
		}
	}
		
	printf("\n\n");
	i=0;
	for(i=0;i<tam;i++)
	{
		printf("%d \n",a[i]);		
	}	

	return 0;
}
	

